  [1m[35mSQL (0.4ms)[0m  INSERT INTO "entries" ("created_at", "feed_id", "guid", "json", "link", "published_at", "title", "updated_at") VALUES ($1, $2, $3, $4, $5, $6, $7, $8) RETURNING "id"  [["created_at", Thu, 04 Jun 2015 17:23:40 UTC +00:00], ["feed_id", 5], ["guid", "http://www.huffingtonpost.com/2015/06/04/foot-shape-personality_n_7512364.html"], ["json", "{\"title\":\"What Your Foot Shape Says About Your Personality\",\"link\":\"http://www.huffingtonpost.com/2015/06/04/foot-shape-personality_n_7512364.html\",\"author\":\"David Moye\",\"description\":\"If you thought a person's personality could be best ascertained by the way they express themselves verbally, you are sadly mistaken.\\u003Cbr /\\u003E\\n\\u003Cbr /\\u003E\\nApparently, we should have been looking at the feet all along.\\u003Cbr /\\u003E\\n\\u003Cbr /\\u003E\\n\\u003Cbr /\\u003E\\n\\u003CMETA HTTP-EQUIV=\\\"refresh\\\" CONTENT=\\\"3;URL=http://pubx.ch/1H4V7MR\\\"\\u003E \\u003Cp\\u003E-- This feed and its contents are the property of The Huffington Post, and use is subject to our \\u003Ca href='http://www.huffingtonpost.com/terms.html/'\\u003Eterms.\\u003C/a\\u003E It may be used for personal consumption, but may not be distributed on a website.\\u003C/p\\u003E\",\"pubDate\":\"2015-06-04T10:13:46-07:00\",\"guid\":\"http://www.huffingtonpost.com/2015/06/04/foot-shape-personality_n_7512364.html\"}"], ["link", "http://www.huffingtonpost.com/2015/06/04/foot-shape-personality_n_7512364.html"], ["published_at", Thu, 04 Jun 2015 17:13:46 UTC +00:00], ["title", "What Your Foot Shape Says About Your Personality"], ["updated_at", Thu, 04 Jun 2015 17:23:40 UTC +00:00]]
